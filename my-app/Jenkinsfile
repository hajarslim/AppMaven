pipeline {
  agent any
  stages { 
    dir("my-app"){
    stage ('Clean') {
      steps {
        
        sh 'mvn clean' 
      } 
    } 
    stage ('Test') { 
      steps {
        sh 'mvn test' 
      } 
    
    } 
    stage('Package') { 
      steps {
        sh 'mvn package' 
       }
    }
    }
  }
}
